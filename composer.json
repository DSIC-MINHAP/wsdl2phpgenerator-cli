{
    "name": "wsdl2phpgenerator/wsdl2phpgenerator-cli",
    "description": "Simple CLI utility for generating php classes from a wsdl file.",
    "keywords": ["soap", "wsdl"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Wsdl2PhpGenerator\\": ["src/", "tests/"]
        }
    },
    "bin": ["wsdl2php"],
    "repositories": [{
            "type": "git",
            "url": "https://github.com/DSIC-MINHAP/wsdl2phpgenerator"
        }
    ],
    "config": {
        "github-oauth": {
            "github.com": "854cfda846773638e5be58eb529f4079db8ae559"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "ext-soap": "*",
        "wsdl2phpgenerator/wsdl2phpgenerator": "@dev",
        "psr/log": "1.0.*",
        "symfony/console": "~2.1"
    },
    "require-dev": {
        "kasperg/phing-github": "0.2.*",
        "kherge/box": "^2.7",
        "phing/phing": "2.7.*",
        "phpunit/phpunit": "4.0.*"
    }
}
